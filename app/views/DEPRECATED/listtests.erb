<h1> TESTS </h1>


<% found=false %>
<% @tests.each do |hash| %>
  <% found=true %>
  <% guid = hash.keys.first; value = hash[guid] %>
  <b> <%= guid %> </b> (<a href="<%= value["api"] %>">API</a>): 
  <span title="<%= value["description"] %>"> <%= value["title"] %> </span>
<% end %>
<% unless found %>
<b>NO TESTS REGISTERED</b>
<% end %>
<br/><br/>

